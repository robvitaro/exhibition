= form_for [@user, @work] do |f|
  .form-group
    =f.label :name
    =f.text_field :name, class: 'form-control'
  .form-group
    =f.label :description
    =f.text_area :description, class: 'form-control'
  .form-group
    =f.label :images
    =f.file_field :images, multiple: true, class: 'form-control'
  =f.submit class: 'btn btn-primary'

